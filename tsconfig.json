{
  "include": ["src", "src/struct/utils/Canvas/node-canvas-with-twemoji.d.ts"],
  "compilerOptions": {
    "rootDir": "src",
    "outDir": "dist",
    "module": "ES6",
    "target": "es6",
    "moduleResolution": "node",
    "importHelpers": false,
    "sourceMap": false,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictBindCallApply": true,
    "strictPropertyInitialization": false,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "pretty": true,
  }
}